<?xml version="1.0" encoding="UTF-8"?>
<database name="SupOrm">
	<table name="users">
		<column name="id" type="INTEGER"/>
		<column name="pseudo" type="CHAR(255)"/>
		<column name="password" type="CHAR(255)"/>
		<column name="age" type="INTEGER"/>
		<column name="gender" type="BOOL"/>
	</table>
	<table name="messages">
		<column name="id" type="INTEGER"/>
		<column name="sender" type="CHAR(255)"/>
		<column name="receiver" type="CHAR(255)"/>
		<column name="content" type="CHAR(255)"/>
	</table>
</database>